<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/arkaPlanRengi"
    tools:context=".ActivitySekiller">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="69dp"
        android:layout_height="77dp"
        app:layout_constraintEnd_toStartOf="@+id/imageView3"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/dikdortgen"
        tools:layout_editor_absoluteY="85dp" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="81dp"
        android:layout_height="83dp"
        app:layout_constraintEnd_toStartOf="@+id/imageView4"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/imageView2"
        app:srcCompat="@drawable/cember"
        tools:layout_editor_absoluteY="85dp" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="92dp"
        android:layout_height="90dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/imageView3"
        app:srcCompat="@drawable/ucgen"
        tools:layout_editor_absoluteY="86dp" />

    <TextView
        android:id="@+id/textViewKare"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Alan"
        android:textSize="18sp"
        app:layout_constraintEnd_toStartOf="@+id/textViewCember"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="203dp" />

    <TextView
        android:id="@+id/textViewCember"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Alan"
        android:textSize="18sp"
        app:layout_constraintEnd_toStartOf="@+id/textViewUcgen"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/textViewKare"
        tools:layout_editor_absoluteY="205dp" />

    <TextView
        android:id="@+id/textViewUcgen"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Alan"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/textViewCember"
        tools:layout_editor_absoluteY="208dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        tools:layout_editor_absoluteX="15dp"
        tools:layout_editor_absoluteY="266dp" />

</androidx.constraintlayout.widget.ConstraintLayout>